<section class="section-berita mt-5">
    <div class="container">
        <h2 class="py-3">Berita Terbaru</h2>
        <div class="row mt-5">
            {%- for post in collections.featured -%}
                <div class="col-12 col-md-6 col-lg-4" data-aos="fade-right">
                    {% set title = post.data.title %}
                    {% set description = post.data.description %}
                    {% set author = post.data.author %}
                    {% set date = post.date %}
                    {% include 'snippet.njk' %}
                </div>
            {%- endfor -%}
        </div>
        <div class="row mb-5">
            <div class="col d-flex justify-content-center">
                <button class="btn btn-primary lainya-btn" data-aos="fade-right">Lainya</button>
            </div>
        </div>
    </div>
</section>